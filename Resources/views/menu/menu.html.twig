{% trans_default_domain 'mweb' %}<nav class="navbar navbar-inverse" style="position:fixed;top:0;z-index:99999;width:100%;">    <ul class="nav navbar-nav">        {% set logo = "admin.main.logo"|trans({}, 'mweb') %}        {% set logoName = "bundles/mwebadmin/img/"~logo %}        <li ><a id="mweb-menu-logo" target="_blank" class="no-padding" href="{{ "admin.main.companyLink"|trans({}, 'mweb') }}" ><img src="{{ asset(logoName) }} "/>  </a></li>        <li ><a href="{{ path('mweb_admin') }}"><span class="glyphicon glyphicon-home"></span>  {{ 'admin.project'|trans({}, "mweb") }} - ADMIN</a></li>        {% for menuKey, menu in adminMenu %}            <li class="dropdown">                <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">{{menu.beautyName}}<span class="caret"></span></a>                <ul class="dropdown-menu">                    {% for key, entity in adminEntities %}                        {% if entity.menu == menuKey %}                            <li><a href="{{ path('mweb_admin_list_entity',{'entityAlias': key}) }}">{{ 'admin.seeList'|trans({}, "mweb") }}</a></li>                            <li role="separator" class="divider"></li>                            <li><a href="{{ path('mweb_admin_edit_entity',{'_locale': getDefaultLocal() ,'entityAlias': key, 'id': 'new'}) }}">{{ 'admin.add'|trans({}, "mweb") }}</a></li>                        {% endif %}                    {% endfor %}                </ul>            </li>        {% endfor %}        <li class="dropdown">            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">                <span class="glyphicon glyphicon-user"></span>  {{ 'admin.menu.users'|trans }}                <span class="caret"></span>            </a>            <ul class="dropdown-menu">                <li ><a href="{{ path('fos_user_profile_edit')  }}">{{ 'admin.menu.myAccount'|trans }}</a></li>                <li role="separator" class="divider"></li>                <li ><a href="{{ path('mweb_admin_user_list')  }}">{{ 'admin.menu.listUsers'|trans }}</a></li>                {#<li role="separator" class="divider"></li>                <li ><a href="{{ path('mweb_admin_user_edit', {"userId" : 'new'})  }}">{{ 'admin.menu.addUser'|trans }}</a></li>#}            </ul>        </li>        <li ><a href="{{ path('lexik_translation_overview')  }}"><span class="glyphicon glyphicon-globe"></span> {{ 'admin.menu.trad'|trans }}</a></li>    </ul>    <div class="navbar-collapse">        <ul class="nav navbar-nav navbar-right">            {% if is_granted('ROLE_SUPER_ADMIN') %}                {% if mw_status.confValue == 'offline' %}                    <li ><a style="color:#f00;" href="{{ path('mweb_admin_update_status_website')  }}"> Site hors ligne <span class="glyphicon glyphicon-lock"></span></a></li>                {% else %}                    <li><a href="{{ path('mweb_admin_update_status_website')  }}"><span class="glyphicon glyphicon-lock"></span></a></li>                {% endif %}                <li><a href="{{ path('mweb_admin_update_cache')  }}"><span class="glyphicon glyphicon-refresh"></span></a></li>            {% endif %}            <li ><a target="_blank" href="{{ path('_homepage', {'_locale': getDefaultLocal() })  }}"><span class="glyphicon glyphicon-share-alt"></span> {{ 'admin.menu.gotoWebsite'|trans }} </a></li>            <li><a class="elm-menu-link" href="{{ path('fos_user_security_logout') }}"><span class="glyphicon glyphicon-off"></span>  {{ 'admin.menu.logout'|trans }}</a></li>        </ul>    </div></nav>